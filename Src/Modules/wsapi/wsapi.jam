SubDir LUAPLUS Src Modules wsapi ;

IncludeModule copyfile ;

{

local SRCS =
		src/wsapi/cgi.lua
		src/wsapi/common.lua
		src/wsapi/fastcgi.lua
		src/wsapi/request.lua
		src/wsapi/response.lua
		src/wsapi/ringer.lua
		src/wsapi/sapi.lua
		src/wsapi/util.lua
		src/wsapi/xavante.lua
;

LuaPlusAddSimpleModule wsapi : $(SRCS) ;

local filename ;
for filename in $(SRCS)
{
	CopyFile wsapi : $(LUA_LDIR)/wsapi/$(filename:BS) : $(SUBDIR)/$(filename) ;
}

}
